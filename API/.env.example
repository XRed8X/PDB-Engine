# Path to the PDB Engine binary
PDBENGINE_BINARY_PATH=/path/to/UniDesign

# Directory configuration
WORKING_DIR=pdbengine_jobs
OUTPUT_FOLDER_NAME=pdbengine_files

# Execution configuration
PDBENGINE_TIMEOUT=600

# File configuration
MAX_FILE_SIZE=104857600
ALLOWED_EXTENSIONS=[".pdb",".PDB"]
MAX_CONCURRENT_TASKS=3

# API configuration
API_TITLE=PDB Engine API
API_DESCRIPTION=API to interact with the PDB Engine for molecular structure processing.
API_VERSION=1.0.0

# Server configuration
HOST=0.0.0.0
PORT=8000

# CORS configuration (comma-separated list of allowed origins)
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]

# Logging configuration
LOG_LEVEL=INFO
